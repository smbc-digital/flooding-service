{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Warning"
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "C:\\Logs\\flooding_service\\log-.log",
          "rollingInterval": "Day"
        }
      }
    ],
    "Enrich": [
      "FromLogContext"
    ]
  },
  "ElasticSearchAwsConfiguration": {
    "Region": "eu-west-1",
    "IndexFormat": "flooding-service-local-{0:yyyy.MM-dd}",
    "InlineFields": false,
    "MinimumLevel": "Warning",
    "Enabled": false,
    "Url": "https://search-stockport-logs-4evarozpqvlofjsnxzliq7eqdy.eu-west-1.es.amazonaws.com/"
  },
  "Availability": {
    "BaseUrl": "http://scnavailprod.stockport.gov.uk/api/v1",
    "ErrorRoute": "/error/500",
    "WhitelistedRoutes": [
      "/swagger/index.html"
    ],
    "Environment": "local",
    "AllowSwagger": true
  },
  "IVerintServiceGatewayConfig": {
    "baseUrl": "http://scnwebdev1.stockport.gov.uk:8050/"
  },
  "IMailingServiceGatewayConfig": {
    "baseUrl": "http://localhost:5500/"
  },
  "PavementVerintOptions": {
    "EventCode": 2002592,
    "Classification": "Public Realm >> Highways >> Flooded Roadway",
    "EventTitle": "Flooded pavement"
  },
  "ConfirmAttributeFormOptions": {
    "EventId": [
      {
        "type": "pavement",
        "value": "2002592"
      }
    ],
    "ServiceCode": [
      {
        "type": "pavement",
        "value": "HWAY"
      }
    ],
    "SubjectCode": [
      {
        "type": "pavement",
        "value": "CWFD" 
      }
    ],
    "ClassCode": "SERV", 
    "RiverOrCulvertedWaterConfig": [
      {
        "type": "river",
        "code": "CONF_ATTRIBUTE_FSRC_CODE",
        "value": "RIV"
      },
      {
        "type": "culverted",
        "code": "CONF_ATTRIBUTE_FSRC_CODE",
        "value": "CULV"
      }
    ]
  }
}